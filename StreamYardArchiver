function removeOldest(){
    if(document.getElementsByTagName('button').length>5){
        console.log(document.getElementsByTagName('button').length);
        document.getElementsByTagName('button')[document.getElementsByTagName('button').length-1].click(); 
        setTimeout(()=>{
            document.getElementsByTagName('button')[document.getElementsByTagName('button').length-1].click(); 
            setTimeout(()=>{
                document.getElementsByTagName('button')[document.getElementsByTagName('button').length-1].click(); 
                setTimeout(()=>{
                    removeOldest();
                },5000)
            },1000)
        },1000)
    }
}
